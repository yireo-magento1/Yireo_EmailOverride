    "description": "Override Magento emails in your app/design/frontend/PACKAGE/THEME/locale folder",
    "authors": [
        {
            "name": "Jisse Reitsma (Yireo)",
            "email": "jisse@yireo.com"
        }
    ],
    "require": {
        "php": ">=5.3.0"
    },
    "require-dev":{
        "phpunit/phpunit":"*",
        "composer/composer":"*@dev"
    },
    "repositories": [
        {
            "type": "composer",
